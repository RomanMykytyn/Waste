{
    "name": "just-digital-server",
    "version": "1.0.41",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "forever -o ./output.log -e ./error.log start -c 'node ' ./dist/src/index.js",
        "start-regular": "node ./dist/src/index.js",
        "build": "tsc",
        "dev": "concurrently --kill-others 'npm run dev:server' 'npm run ts-watch'",
        "dev-debug": "concurrently --kill-others 'npm run dev:server-debug' 'npm run ts-watch'",
        "dev:server": "nodemon ./dist/src/index.js",
        "dev:server-debug": "nodemon --inspect ./dist/src/index.js",
        "ts-watch": "tsc -w",
        "logs": "forever logs",
        "list": "forever list",
        "stop": "forever stopall",
        "patch-version": "npm --no-git-tag-version version patch",
        "commit": "git commit -a",
        "release": "VER=\"$(npm run patch-version | tail -1)\" && npm run commit && git tag $VER && git push --tags && git push origin HEAD"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@feathersjs/configuration": "^2.0.4",
        "@feathersjs/errors": "^3.3.4",
        "@feathersjs/express": "^1.2.7",
        "@feathersjs/feathers": "^3.2.3",
        "@sendgrid/mail": "^6.3.1",
        "@types/express": "^4.16.0",
        "@types/feathersjs__configuration": "^1.0.1",
        "@types/feathersjs__errors": "^3.2.1",
        "@types/feathersjs__express": "^1.1.4",
        "@types/feathersjs__feathers": "^3.0.5",
        "@types/node": "^10.11.4",
        "@types/request-promise": "^4.1.42",
        "concurrently": "^4.0.1",
        "cors": "^2.8.4",
        "cron": "^1.5.0",
        "crypto-js": "^3.1.9-1",
        "csv-parse": "^3.1.3",
        "express": "^4.16.3",
        "expressjs": "^1.0.1",
        "facebook-nodejs-business-sdk": "^3.2.4",
        "feathers-hooks-common": "^4.17.10",
        "forever": "^1.0.0",
        "googleapis": "^41.0.1",
        "graphql": "^14.0.2",
        "lodash": "^4.17.15",
        "moment": "^2.22.2",
        "mysql2": "^1.6.5",
        "nanoid": "^3.1.31",
        "node-adwords": "^201809.0.1",
        "nodemon": "^1.18.4",
        "promise-mysql": "^3.3.1",
        "request": "^2.88.0",
        "request-promise": "^4.2.2",
        "sequelize": "^6.0.0",
        "typescript": "^3.1.1"
    },
    "devDependencies": {
        "tslint-sonarts": "^1.9.0"
    }
}
